{
  "name": "pu-client",
  "version": "1.0.14",
  "description": "pocket universe client",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/app.ts",
    "login": "tsc && node ./dist/loginApp.js",

    "update": "git fetch && git pull",

    "start": "tsc && pm2 start ./dist/app.js --name pu-client",
    "restart": "tsc && pm2 restart ./dist/app.js --name pu-client",
    "stop": "pm2 stop dist/app.js --name pu-client",
    "delete": "pm2 delete dist/app.js --name pu-client",
    "monitor": "pm2 monit dist/app.js --name pu-client",
    "logs": "pm2 logs --name pu-client"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "chalk": "^4.1.2",
    "copy-paste": "^1.5.3",
    "enquirer": "^2.4.1",
    "node-schedule": "^2.1.1",
    "pu-client": "^1.0.19",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "@types/node-schedule": "^2.1.5",
    "@types/lodash": "^4.14.202",
    "typescript": "^5.1.6"
  }
}
